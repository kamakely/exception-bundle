{
    "name": "tounaf/exception-bundle",
    "description": "Exception handler for symfony project",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "symfony/dependency-injection": "6.4.* || 6.3.* || 6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "symfony/config": "6.4.* || 6.3.* || 6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "symfony/framework-bundle": "6.4.* || 6.3.* || 6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "symfony/http-kernel": "6.4.* || 6.3.* || 6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "tounaf/exception-handler": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Tounaf\\ExceptionBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tounaf\\ExceptionBundle\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Fetra Harinjatovo",
            "email": "harinjatovo.fetra@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/test-pack": "^1.1",
        "symfony/var-dumper": "^6.4 || ^6.3 || ^6.2",
        "squizlabs/php_codesniffer": "*",
        "friendsofphp/php-cs-fixer": "^3.39",
        "enlightn/security-checker": "^1.11",
        "rector/rector": "^1.0",
        "symfony/maker-bundle": "^1.48"
    }
}
